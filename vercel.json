{
  "version": 2,
  "name": "visla",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "home.css", "use": "@vercel/static" },
    { "src": "index.js", "use": "@vercel/static" },
    { "src": "assets/images/**", "use": "@vercel/static" }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/index.html" }],
  "headers": [
    {
      "source": "/home.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/index.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
